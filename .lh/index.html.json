            "content": "<!DOCTYPE html>\r\n<html lang=\"en\">\r\n\r\n<head>\r\n    <meta charset=\"UTF-8\" />\r\n    <meta http-equiv=\"X-UA-Compatible\" content=\"IE=edge\" />\r\n    <meta name=\"viewport\" content=\"width=device-width, initial-scale=1.0\" />\r\n    <title>Document</title>\r\n    <link href=\"https://cdn.jsdelivr.net/npm/bootstrap@5.2.0-beta1/dist/css/bootstrap.min.css\" rel=\"stylesheet\" integrity=\"sha384-0evHe/X+R7YkIZDRvuzKMRqM+OrBnVFBL6DOitfPri4tjfHxaWutUpFmBp4vmVor\" crossorigin=\"anonymous\" />\r\n</head>\r\n\r\n<body ng-app=\"lab1\">\r\n    <div ng-controller=\"student\" class=\"container\">\r\n        <!-- b1 -->\r\n        <div class=\"b1\">\r\n            <h1>Bài 1</h1>\r\n            <img src=\"./upload/{{image}}\" alt=\"\" width=\"100px\" height=\"100px\" class=\"img-fluid\" />\r\n            <p>Họ và tên : {{fullname}}</p>\r\n            <p>Ngày sinh : {{data}}</p>\r\n            <p>Giới tính : {{gender}}</p>\r\n            <p>Điểm trung bình : {{toal}}</p>\r\n        </div>\r\n        <!-- b2 -->\r\n        <div class=\"b2\">\r\n            <h1>Bài2</h1>\r\n            <img src=\"./upload/{{student.image}}\" alt=\"\" width=\"100px\" height=\"100px\" class=\"img-fluid\" />\r\n            <p>Họ và tên : {{student.fullname}}</p>\r\n            <p>Ngày sinh : {{student.data}}</p>\r\n            <p>Giới tính : {{student.gender}}</p>\r\n            <p>Điểm trung bình : {{student.toal}}</p>\r\n        </div>\r\n        <!-- b3 -->\r\n        <div class=\"b3\">\r\n            <h1>Bài 3</h1>\r\n            <table class=\"table\">\r\n                <thead class=\"thead-dark\">\r\n                    <tr>\r\n                        <th scope=\"col\"></th>\r\n                        <th scope=\"col\">Họ và tên</th>\r\n                        <th scope=\"col\">Ngày sinh</th>\r\n                        <th scope=\"col\">Giới tính</th>\r\n                        <th scope=\"col\">Điểm</th>\r\n                    </tr>\r\n                </thead>\r\n                <tbody>\r\n                    <tr>\r\n                        <th scope=\"row\">\r\n                            <img src=\"./upload/{{student[0].image}}\" alt=\"\" width=\"100px\" height=\"100px\" />\r\n                        </th>\r\n                        <td>{{student[0].fullname}}</td>\r\n                        <td>{{student[0].data}}</td>\r\n                        <td>{{student[0].gender}}</td>\r\n                        <td>{{student[0].toal}}</td>\r\n                    </tr>\r\n                    <tr>\r\n                        <th scope=\"row\">\r\n                            <img src=\"./upload/{{student[1].image}}\" alt=\"\" width=\"100px\" height=\"100px\" />\r\n                        </th>\r\n                        <td>{{student[1].fullname}}</td>\r\n                        <td>{{student[1].data}}</td>\r\n                        <td>{{student[1].gender}}</td>\r\n                        <td>{{student[1].toal}}</td>\r\n                    </tr>\r\n                </tbody>\r\n            </table>\r\n        </div>\r\n        <!-- b4 -->\r\n        <div class=\"b4\">\r\n            <h1>Bài 4</h1>\r\n            <form action=\"\">\r\n                Chiều dài :\r\n                <input type=\"text\" ng-model=\"cd\" class=\"form-control\" /><br /> Chiều rộng : <input type=\"text\" ng-model=\"cr\" class=\"form-control\" />\r\n                <br /> Diện tích :\r\n                <label ng-bind=\"dt\"></label> chu vi :\r\n                <label ng-bind=\"cv\"></label><br />\r\n                <button type=\"button\" ng-click=\"tinh()\">Tính</button>\r\n            </form>\r\n        </div>\r\n        <!-- b5 -->\r\n        <div class=\"b5\">\r\n            <form action=\"\">\r\n                Họ và tên:\r\n                <input type=\"text\" ng-model=\"student.name\" class=\"form-control\" /><br /> Ngày sinh :\r\n                <input type=\"text\" ng-model=\"student.ns\" class=\"form-control\" /><br /> Điểm :\r\n                <input type=\"text\" ng-model=\"student.mark\" class=\"form-control\" /><br /> Giới tính :\r\n                <select ng-model=\"student.gt\" class=\"form-control\">\r\n            <option value=\"\">nam</option>\r\n            <option value=\"\">nữ</option></select\r\n          ><br />\r\n\r\n          <button type=\"button\" ng-click=\"submit()\">Submit</button>\r\n        </form>\r\n        <p>Họ và tên{{student.name}}</p>\r\n        <p>Ngày Sinh{{student.ns}}</p>\r\n        <p>Giới tính{{student.gt}}</p>\r\n        <p>Điểm{{student.mark}}</p>\r\n        Kết quả :\r\n        <p ng-bind=\"student.hl\"></p>\r\n      </div>\r\n    </div>\r\n\r\n    <script src=\"https://ajax.googleapis.com/ajax/libs/angularjs/1.8.2/angular.min.js\"></script>\r\n    <script>\r\n      const app = angular.module(\"lab1\", []);\r\n      app.controller(\"student\", function ($scope) {\r\n        $scope.image = \"1.png\";\r\n        $scope.fullname = \"Nguyễn Phúc Thành\";\r\n        $scope.data = \"23/08/2001\";\r\n        $scope.gender = \"Nam\";\r\n        $scope.toal = \"7.9\";\r\n        // $scope.student = {\r\n        //   image: \"1.png\",\r\n        //   fullname: \"Nguyễn Phúc Thành2\",\r\n        //   data: \"23/08/2001\",\r\n        //   gender: \"Nam\",\r\n        //   toal: \"7.9\",\r\n        // };\r\n        $scope.student = [\r\n          {\r\n            image: \"1.png\",\r\n            fullname: \"Nguyễn Phúc Thành 1\",\r\n            data: \"24/08/2001\",\r\n            gender: \"Nam\",\r\n            toal: \"7.9\",\r\n          },\r\n          {\r\n            image: \"1.png\",\r\n            fullname: \"Nguyễn Phúc Thành 2\",\r\n            data: \"23/08/2001\",\r\n            gender: \"Nam\",\r\n            toal: \"8\",\r\n          },\r\n        ];\r\n        $scope.tinh = function () {\r\n          var a = parseFloat($scope.cd);\r\n          var b = parseFloat($scope.cr);\r\n          $scope.dt = a * b;\r\n          $scope.cv = (a + b) * 2;\r\n        };\r\n        $scope.submit = function () {\r\n          var infomation = parseFloat($scope.student.mark);\r\n          if (infomation < 5) {\r\n            $scope.student.hl = \"Trượt\";\r\n          } else {\r\n            $scope.student.hl = \"Đỗ\";\r\n          }\r\n        };\r\n      });\r\n    </script>\r\n  </body>\r\n</html>"
